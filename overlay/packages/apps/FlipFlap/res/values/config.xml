<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Define Device Lid Style

     1 HTC Style Dotcase
     2 Asus/LG Style Circle Window Cover

     For example, a device with Asus Circle Cover would set 2 -->
    <integer name="config_deviceCoverType">2</integer>

        <!-- Set to true for devices which have covers that block
     proximity sensor. This will disable prox wake check while cover is closed -->
    <bool name="config_needsProxWakeDisable">true</bool>
</resources>
